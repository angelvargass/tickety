<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Ticket.
    -->
    <changeSet id="20230321021403-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="user_account_id"
                                 baseTableName="ticket"
                                 constraintName="fk_ticket__user_account_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_account"/>

        <addForeignKeyConstraint baseColumnNames="event_id"
                                 baseTableName="ticket"
                                 constraintName="fk_ticket__event_id"
                                 referencedColumnNames="id"
                                 referencedTableName="event"/>
    </changeSet>
</databaseChangeLog>
